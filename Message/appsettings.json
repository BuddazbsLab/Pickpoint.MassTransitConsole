{
  "Rabbit": {
    "username": "guest",
    "password": "guest",
    "host": "localhost",
    "port": "5672",
    "queueName": "event-listener",
    "numberMessage": "100",
    "numberListener": "1",
    "messageSendTimeIntervalSeconds": 10,
    "trafficSendinBytes": 1000,
    "alphabet": "ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789"
  },
  "NLog": {
    "autoReload": true,
    "throwConfigExceptions": true,
    "internalLogLevel": "Off",
    "targets": {
      "console": {
        "type": "Console",
        "layout": "${message}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "console"
      }
    ]
  }
}
